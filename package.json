    "@babel/runtime-corejs2": "^7.3.4",
    "autoprefixer": "^9.4.5",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "http-server": "^0.11.1",
    "image-webpack-loader": "^4.6.0",
    "imagemin": "^6.1.0",
